                    <Mapping field="MultDiv">
                        <To object="CuryRateRecordsEffDate" field="CuryMultDiv" />
                    </Mapping>
                    <Mapping field="CurrencyRate">
                        <To object="CuryRateRecordsEffDate" field="CuryRate" />
